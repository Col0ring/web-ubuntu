{
  "name": "web-ubuntu",
  "description": "A website simulating Ubuntu's GUI",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Col0ring",
    "email": "1561999073@qq.com"
  },
  "keywords": [
    "react",
    "typescript",
    "web-ubuntu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Col0ring/web-ubuntu.git"
  },
  "scripts": {
    "prepare": "husky install",
    "pre-commit:lint-staged": "lint-staged",
    "commit-msg:commitlint": "commitlint --config commitlint.config.js -e $HUSKY_GIT_PARAMS",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prettier": "prettier --write '**/*.{ts,tsx,js,json,html,yml,css,less,scss,md}'",
    "lint": "tsc && npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier ",
    "lint:prettier": "npm run prettier",
    "lint:eslint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx,.js,.jsx .",
    "lint:stylelint": "stylelint --fix --config stylelint.config.js **/*.{less,css,scss}",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "autoprefixer": "^10.3.1",
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "immer": "^9.0.5",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.2.0",
    "postcss": "^8.3.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-use-methods": "^1.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^5.1.0",
    "tailwindcss": "^2.2.15"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.1.1",
    "@col0ring/vite-plugin-copy": "^1.0.1",
    "@col0ring/vite-plugin-mock": "^1.0.3",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.9.4",
    "@types/react": "^17.0.22",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-less": "^1.0.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vite-plugin-eslint": "^1.3.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "chrome > 60"
  ]
}
